h.Text = "Currently not playing anything"

function pau()
	for i,v in pairs(game.Players.LocalPlayer.Character:GetChildren()) do
		if v.Name == "MusicItem" or v.Name == "MusicItem2" and v.ClassName == "Part" then
			v:Remove()
			fold.Sound:Pause()
			fold.Sound.TimePosition = 0
		end
	end
end

function coru()
	for i,v in pairs(game.Players.LocalPlayer.Character:GetChildren()) do
		if v.Name == "MusicItem" or v.Name == "MusicItem2" and v.ClassName == "Part" then
			v:Remove()
		end
	end
end

function loadPart(text)
	for i,v in pairs(game.Players.LocalPlayer.Character:GetChildren()) do
		if v.Name == "MusicItem" or v.Name == "MusicItem2" and v.ClassName == "Part" then
			v:Remove()
			fold.Sound:Pause()
			fold.Sound.TimePosition = 0
			h.Text = "Loading..."
			wait(0.1)
		end
	end
	local Part = Instance.new("Part", game.Players.LocalPlayer.Character)
	Part.CanCollide = false
	Part.Shape = "Block"
	Part.Anchored = false
	Part.Locked = true
	Part.Name = "MusicItem"
	Part.Transparency = 0
	Part.Material = "Neon"
	Part.Color = Color3.fromRGB(0, 0, 0) --Cool Colors CGA brown and Royal purple

	local Part2 = Instance.new("Part", game.Players.LocalPlayer.Character)
	Part2.CanCollide = false
	Part2.Shape = "Block"
	Part2.Anchored = false
	Part2.Locked = true
	Part2.Name = "MusicItem2"
	Part2.Transparency = 0.8
	Part2.Material = "Neon"
	Part2.Color = Color3.fromRGB(0, 0, 0) --Cool Colors CGA brown and Royal purple


	Sound.SoundId = text
	Sound.EmitterSize = math.huge
	Sound.MaxDistance = math.huge
	Sound.Looped = true
	Sound.Pitch = 1
	Sound.Name = "Sound"
	Sound.Volume = 10
	Sound:Play()

	Part.CFrame = CFrame.new(game.Players.LocalPlayer.Character.HumanoidRootPart.Position) * CFrame.new(0,-2.9,0)
	local bp = Instance.new("BodyPosition", Part)
	bp.Position = Character:FindFirstChild("HumanoidRootPart").CFrame * CFrame.new(0, -2.9, 0).p
	bp.MaxForce = Vector3.new(10000,10000,10000)
	bp.P = 100000
	Part2.CFrame = CFrame.new(game.Players.LocalPlayer.Character.HumanoidRootPart.Position) * CFrame.new(0,-2.9,0)
	local bp2 = Instance.new("BodyPosition", Part2)
	bp2.Position = Character:FindFirstChild("HumanoidRootPart").CFrame * CFrame.new(0, -2.9, 0).p
	bp2.MaxForce = Vector3.new(10000,10000,10000)
	bp2.P = 100000
	x=0
	game:GetService("RunService").RenderStepped:Connect(function()
		bp.Position = Character:FindFirstChild("HumanoidRootPart").CFrame * CFrame.new(0, -2.9, 0).p
		bp2.Position = Character:FindFirstChild("HumanoidRootPart").CFrame * CFrame.new(0, -2.9, 0).p
		local alg = math.floor(Sound.PlaybackLoudness)/50
		Part.Size = Vector3.new(.6+alg,.01,.6+alg)
		Part2.Size = Vector3.new(6+alg,.01,6+alg)
		Part.Color = Color3.fromRGB(20*alg, 0, 0)
		Part.Orientation = Vector3.new(0, math.random(-180, 180), 0)
		Part2.Orientation = Vector3.new(0, math.random(-180, 180), 0)
		f.BackgroundColor3 = Color3.fromRGB(7*alg, 7*alg, 7*alg)
		sf.BackgroundColor3 = Color3.fromRGB(7*alg, 7*alg, 7*alg)
		TextLabel.BackgroundColor3 = Color3.fromRGB(7*alg, 7*alg, 7*alg)
		dist.BackgroundColor3 = Color3.fromRGB(7*alg, 7*alg, 7*alg)
		mtext.TextColor3 = Color3.fromRGB(20*alg, 5*alg, 0)
		nameText.TextColor3 = Color3.fromRGB(20*alg, 0, 0)
		nameText.Rotation = math.random(-Sound.PlaybackLoudness/100*math.random(1,3),Sound.PlaybackLoudness/100*math.random(1,2))
		h.Text = r(8).. "          " ..Sound.PlaybackLoudness.. "        " ..rrev(8)
		if guiShake == true then
			for i,v in next, game.Players.LocalPlayer.PlayerGui:GetDescendants() do
				pcall(function()
					if v:IsA("Frame") or v:IsA("TextLabel") or v:IsA("TextBox") or v:IsA("TextButton") or v:IsA("ScrollingFrame") or v:IsA("ImageLabel") or v:IsA("ImageButton") and not v:IsA("ScreenGui") then
						v.Rotation = math.random(-Sound.PlaybackLoudness/100*math.random(1,3),Sound.PlaybackLoudness/100*math.random(1,2))
					elseif v:FindFirstChildWhichIsA("ScreenGui") then
					end
				end)
			end
			for i,v in next, game.CoreGui.BubbleChat:GetDescendants() do
				pcall(function()
					if v:IsA("Frame") or v:IsA("TextLabel") or v:IsA("TextBox") or v:IsA("TextButton") or v:IsA("ScrollingFrame") or v:IsA("ImageLabel") or v:IsA("ImageButton") and not v:IsA("ScreenGui") then
						v.Rotation = math.random(-Sound.PlaybackLoudness/100*math.random(1,3),Sound.PlaybackLoudness/100*math.random(1,2))
					elseif v:FindFirstChildWhichIsA("ScreenGui") then
					end
				end)
			end
		else
		end
		if camShake == true then
			pcall(function()
				game.Workspace.CurrentCamera.CFrame = game.Workspace.CurrentCamera.CFrame * CFrame.Angles(0, 
					0, math.rad(math.random(-Sound.PlaybackLoudness, 
						Sound.PlaybackLoudness) * 0.01)) + Vector3.new(
					math.rad(math.random(-Sound.PlaybackLoudness * 0.08, Sound.PlaybackLoudness * 0.08) * 0.05)
				)
			end)
		else
		end
	end)
end
