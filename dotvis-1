	TextLabel_2.TextWrapped = true
	TextLabel_2.TextXAlignment = Enum.TextXAlignment.Left
	TextLabel_2.TextYAlignment = Enum.TextYAlignment.Top

	game:GetService("TweenService"):Create(Notif, TweenInfo.new(1, Enum.EasingStyle.Exponential), {["Position"]=UDim2.new(0.0650000002, 0, 0.926999978, 0)}):Play()
	local S=Instance.new("Sound",game:GetService("SoundService"));S.SoundId="rbxassetid://261072074";S:Play()
	wait(a3)
	game:GetService("TweenService"):Create(Notif, TweenInfo.new(1, Enum.EasingStyle.Exponential), {["Position"]=UDim2.new(-0.0799999982, 0, 0.922999978, 0)}):Play()
	S:Remove()
	wait(1)
	ScreenGui:Remove()
end
function rt(Table) -- Returns a random [...] inside of a table
	return Table[math.random(1,#Table)]
end
function r(Length) -- Generates a random string | Edit if you want
	local NewString = "UNKNOWN-"
	for _=1,Length or math.random(5,100) do
		local Strings = { -- Delete which u want or not
			--string.char(math.random(48,57)), -- 0 to 9
			--string.char(math.random(33,47)), -- Stuff like !@#$%^&*()
			string.char(math.random(65,90)), -- A to Z [ CAPITALS ]
			string.char(math.random(97,122)) -- a to z [ not capitals ]
		}
		NewString = NewString..rt(Strings)
	end
	return NewString or math.random()
end
function rrev(Length) -- Generates a random string | Edit if you want
	local NewString = "-UNKNOWN"
	for _=1,Length or math.random(5,100) do
		local Strings = { -- Delete which u want or not
			--string.char(math.random(48,57)), -- 0 to 9
			--string.char(math.random(33,47)), -- Stuff like !@#$%^&*()
			string.char(math.random(65,90)), -- A to Z [ CAPITALS ]
			string.char(math.random(97,122)) -- a to z [ not capitals ]
		}
		NewString = rt(Strings)..NewString
	end
	return NewString or math.random()
end

