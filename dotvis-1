	TextLabel.BackgroundTransparency = 1.000
	TextLabel.Size = UDim2.new(0, 192, 0, 26)
	TextLabel.Font = Enum.Font.SourceSans
	TextLabel.Text = title
	TextLabel.TextColor3 = a5
	TextLabel.TextScaled = true
	TextLabel.TextSize = 14.000
	TextLabel.TextWrapped = true

	TextLabel_2.Parent = Notif
	TextLabel_2.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
	TextLabel_2.BackgroundTransparency = 1.000
	TextLabel_2.Position = UDim2.new(0, 0, 0.25999999, 0)
	TextLabel_2.Size = UDim2.new(0, 192, 0, 74)
	TextLabel_2.Font = Enum.Font.SourceSans
	TextLabel_2.Text = desc
	TextLabel_2.TextColor3 = a5
